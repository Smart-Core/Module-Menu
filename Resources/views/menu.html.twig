{% set menu = knp_menu_get('MenuModule:MenuBuilder:full', [], {
    'group': group,
    'css_class': css_class,
}) %}

{% if not current_class %}
    {% set current_class = 'current' %}
{% endif %}

{{ knp_menu_render(menu, {
    'currentAsLink': true,
    'currentClass': current_class,
    'compressed': false,
    'depth': null,
}) }}
